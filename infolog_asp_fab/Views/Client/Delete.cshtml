@model infolog_asp_fab.Models.Client

<h2>Supprimer le Client</h2>

<p>Êtes-vous sûr de vouloir supprimer le client <strong>@Model.FirstName @Model.LastName</strong> ?</p>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" asp-for="IdClients" />
    <button type="submit" class="btn btn-danger">Supprimer</button>
    <a asp-action="Index" class="btn btn-secondary">Annuler</a>
</form>
